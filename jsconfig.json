{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "components/*": ["src/components/*"],
      "images/*": ["public/Images/*"],
      "styles/*": ["src/styles/*"],
      "theme/*": ["src/theme/*"],
      "globals/*": ["src/globals/*"],
      "hooks/*": ["src/hooks/*"],
      "api/*": ["src/apiEndpoints/*"],
      "store/*": ["src/store/*"]
    }
  },
  "exclude": ["node_modules"]
}
